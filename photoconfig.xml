<?xml version="1.0"?>
<!-- $Id: photoconfig.xml,v 1.3 1999/11/26 05:28:20 dustin Exp $  -->
<!DOCTYPE config [

	<!ELEMENT config (entry)*>
	<!ELEMENT entry EMPTY>
	<!ELEMENT name (#PCDATA)>
	<!ELEMENT value (#PCDATA)>
	<!ATTLIST entry
		name CDATA #REQUIRED
		value CDATA #REQUIRED>
]
>

<!-- Configuration for the photo album. -->

<config>
	<entry name="dbDriverName" value="postgresql.Driver" />
	<entry name="dbSource" value="jdbc:postgresql://localhost/photo" />
	<entry name="dbUser" value="nobody" />
	<entry name="dbPass" value="" />
	<entry name="objectserver" value="//localhost/RObjectServer" />
	<entry name="imageserver" value="//localhost/ImageServer" />
	<entry name="includes" value="/home/dustin/public_html/jphoto/inc/" />
	<entry name="html_uriroot" value="/~dustin/jphoto/" />
	<entry name="timezone" value="" />
	<entry name="cryptohash" value="SHA" />

	<!-- How long to sleep before doing another store (in minutes) -->
	<entry name="storer_sleep" value="1" />
</config>
