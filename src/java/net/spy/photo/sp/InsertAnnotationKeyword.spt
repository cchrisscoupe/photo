@package
net.spy.photo.sp

@description
Insert a keyword mapping for an annotated region.

@sql
insert into region_keywords_map (
		region_id, word_id
	) values (
		?, ?
	)

@params
annotation_id required INTEGER the ID of the annotated region
word_id required INTEGER the ID of the keyword
